@page "/"

<h1>Hello, world!</h1>

<button @onclick="Klik">Generuj</button>

<ul>
    @foreach (var x in cisla)
    {
        <li>@x</li>
    }
</ul>

@code {
    int[] cisla = { 1, 2, 3 };

    void Klik()
    {
        Array.Resize(ref cisla, Random.Shared.Next(5, 20));
        for (int i = 0; i < cisla.Length; i++)
            cisla[i] = Random.Shared.Next(20);
    }
}